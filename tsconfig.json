{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@layouts/*": [
        "src/layout/*"
      ],
      "@pages/*": [
        "src/pages/*"
      ],
      "@comp/*": [
        "src/components/*"
      ],
      "@icons/*": [
        "src/icons/*"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "preact"
  }
}